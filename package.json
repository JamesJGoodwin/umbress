{
  "name": "umbress",
  "version": "0.0.1a",
  "description": "Dynamic and flexible Express anti-DDoS middleware",
  "main": "index.js",
  "scripts": {
    "build": "run-s test build:pre build:build",
    "watch": "tsc --watch --project ./tsconfig.json",
    "build:pre": "rimraf build",
    "build:build": "tsc --project ./tsconfig.json",
    "test": "jest --detectOpenHandles --config config/jest.json"
  },
  "keywords": [
    "ddos",
    "mitigation",
    "express"
  ],
  "author": "JamesJGoodwin",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.8",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.2"
  }
}
